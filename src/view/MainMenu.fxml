<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
   <children>
      <Label alignment="CENTER" layoutX="150.0" layoutY="14.0" prefHeight="27.0" prefWidth="303.0" text="Scheduling Application">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="appointmentsBtn" layoutX="255.0" layoutY="113.0" mnemonicParsing="false" onAction="#onActionAppointments" text="Appointments" />
      <Label alignment="CENTER" layoutX="150.0" layoutY="57.0" prefHeight="27.0" prefWidth="303.0" text="Main Menu" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="customersBtn" layoutX="255.0" layoutY="149.0" maxWidth="92.0" mnemonicParsing="false" onAction="#onActionCustomers" prefWidth="92.0" text="Customers" />
      <Button fx:id="reportsBtn" layoutX="254.0" layoutY="181.0" maxWidth="92.0" mnemonicParsing="false" onAction="#onActionReports" prefWidth="92.0" text="Reports" />
      <Button fx:id="logoutBtn" layoutX="255.0" layoutY="217.0" maxWidth="92.0" mnemonicParsing="false" onAction="#onActionLogout" prefWidth="92.0" text="Logout" />
   </children>
</AnchorPane>
