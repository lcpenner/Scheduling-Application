<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="headerLabel" alignment="CENTER" layoutX="149.0" layoutY="65.0" prefHeight="27.0" prefWidth="303.0" text="Login to Scheduling Application">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="213.0" layoutY="108.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="213.0" layoutY="160.0" text="Password" />
      <TextField fx:id="usernameTxt" layoutX="213.0" layoutY="125.0" prefHeight="25.0" prefWidth="166.0">
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </TextField>
      <TextField fx:id="passwordTxt" layoutX="213.0" layoutY="177.0" prefHeight="25.0" prefWidth="166.0">
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </TextField>
      <Button fx:id="logInBtn" layoutX="235.0" layoutY="228.0" mnemonicParsing="false" onAction="#onActionLogIn" prefHeight="25.0" prefWidth="118.0" text="Login" />
      <Label fx:id="zoneTxt" layoutX="268.0" layoutY="279.0" text="Not Found" />
      <Label fx:id="loginFailed" layoutX="365.0" layoutY="210.0" prefHeight="62.0" prefWidth="174.0" text=" " textFill="RED" wrapText="true" />
   </children>
</AnchorPane>
